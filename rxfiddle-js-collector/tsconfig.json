{
  "compileOnSave": true,
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "module": "es2015",
    "target": "ES5",
    "strictNullChecks": false,
    "removeComments": true,
    "noImplicitAny": true,
    "declaration": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "lib": [
      "es2015", "dom", "es2015.proxy"
    ]
  },
  "include": [
    "index.ts"
  ],
  "exclude": [
    "node_modules/rxjs"
  ]
}